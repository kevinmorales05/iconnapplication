{
  "name": "iconn",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios:dev": "react-native run-ios --scheme iconnDev --configuration Debug",
    "ios:dev-release": "react-native run-ios --scheme iconnDev --configuration Release",
    "ios:staging": "react-native run-ios --scheme iconnStg --configuration Debug",
    "ios:staging-release": "react-native run-ios --scheme iconnStg --configuration Release",
    "ios:prod": "react-native run-ios --scheme iconn --configuration Debug",
    "ios:prod-release": "react-native run-ios --scheme iconn --configuration Release"
